{
  "AWSEBDockerrunVersion": 1,
  "containerDefinitions": [
    {
      "name": "asbx-org",
      "image": "{$AWS_ACCOUNT_ID}.dkr.ecr.{$AWS_DEFAULT_REGION}.amazonaws.com/asbx-org",
      "hostname": "asbx-org",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8000
        }
      ]
    }
  ]
}